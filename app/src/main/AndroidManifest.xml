<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sunwell.pos_mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sunwell.pos.mobile.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sunwell.pos.mobile.activity.WelcomeActivityTest"/>
        <activity android:name="com.sunwell.pos.mobile.activity.HomeActivity" android:theme="@style/CustomBarTheme"/>
        <activity android:name="com.sunwell.pos.mobile.activity.LoginActivity" />
        <activity android:name="com.sunwell.pos.mobile.activity.LoginActivity2"></activity>
    </application>

</manifest>